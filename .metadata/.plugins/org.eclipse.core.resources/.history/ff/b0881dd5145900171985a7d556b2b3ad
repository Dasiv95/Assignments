package p1;
abstract class Bike{
	Bike(){
		System.out.println("Hello");
	}
	abstract void run();
	void changegear(){
		System.out.println("This lets you change the gear");
	}
}
class Honda extends Bike{
	void run(){
		System.out.println("The honda is running");
	}
}
public class AbsExample {
	void run(){System.out.println("This bike is running smoothly.");}
	public static void main(String args[]){
		Bike b = new Honda();
		b.run();
		
	}

}
